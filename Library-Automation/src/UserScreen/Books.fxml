<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="777.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserScreen.Books_Controller">
    <children>
        <Label fx:id="lblUsernameFromUserScreen" layoutX="14.0" layoutY="14.0" />
        <TableView fx:id="AllBooks" layoutX="14.0" layoutY="73.0" onMouseClicked="#displaySelected" prefHeight="200.0" prefWidth="741.0">
            <columns>
                <TableColumn fx:id="book_id" prefWidth="64.0" text="Book ID" />
                <TableColumn fx:id="book_title" prefWidth="141.0" text="Book Title" />
                <TableColumn fx:id="author_name" prefWidth="171.0" text="Author Name" />
                <TableColumn fx:id="category_name" prefWidth="160.0" text="Category Name" />
                <TableColumn fx:id="book_language" prefWidth="204.0" text="Book Language" />
            </columns>
        </TableView>
        <Button layoutX="133.0" layoutY="9.0" mnemonicParsing="false" onAction="#ShowAllBooks" text="Show Books" />
        <Button layoutX="478.0" layoutY="292.0" mnemonicParsing="false" onAction="#RentBook" text="Rent" />
        <Button layoutX="687.0" layoutY="17.0" mnemonicParsing="false" onAction="#backtomain" text="Back" />
        <TextField fx:id="txtUserid" layoutX="251.0" layoutY="292.0" promptText="Enter User ID" />
      <Label fx:id="lblBookTitle" layoutX="24.0" layoutY="282.0" />
      <Label fx:id="lblBookID" layoutX="24.0" layoutY="313.0" />
    </children>
</AnchorPane>
