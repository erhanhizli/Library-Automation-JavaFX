<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="420.0" prefWidth="966.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserScreen.MyProfile_Controller">
    <children>
        <TableView fx:id="TableProfile" layoutX="9.0" layoutY="61.0" prefHeight="130.0" prefWidth="949.0">
            <columns>
                <TableColumn fx:id="UserID" prefWidth="75.0" text="User ID" />
                <TableColumn fx:id="Name" prefWidth="100.0" text="Name" />
                <TableColumn fx:id="Lastname" prefWidth="127.0" text="Lastname" />
                <TableColumn fx:id="Sex" text="Sex" />
                <TableColumn fx:id="Password" prefWidth="105.0" text="Password" />
                <TableColumn fx:id="Phone" prefWidth="107.0" text="Phone" />
                <TableColumn fx:id="E_Mail" minWidth="2.0" prefWidth="107.0" text="E_Mail" />
                <TableColumn fx:id="RecoveryAnswer" prefWidth="141.0" text="Recovery Answer" />
                <TableColumn fx:id="Username" prefWidth="106.0" text="Username" />
            </columns>
        </TableView>
        <TableView fx:id="TableRentedBooks" layoutX="14.0" layoutY="206.0" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="RentedBooks" prefWidth="199.0" text="Rented Books" />
            </columns>
        </TableView>
        <Button layoutX="224.0" layoutY="14.0" mnemonicParsing="false" onAction="#ShowMyInformation" prefHeight="42.0" prefWidth="168.0" text="Show My Information" />
        <Button fx:id="btnUpdateScreen" layoutX="411.0" layoutY="14.0" mnemonicParsing="false" onAction="#GoToUpdateProfile" prefHeight="42.0" prefWidth="168.0" text="Update Profile" />
        <Button layoutX="595.0" layoutY="14.0" mnemonicParsing="false"  prefHeight="42.0" prefWidth="168.0" text="Show Rented Books" />
        <Button layoutX="224.0" layoutY="249.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="168.0" text="Return Book" />
        <TextField layoutX="224.0" layoutY="206.0" prefHeight="31.0" prefWidth="168.0" promptText="Book Title" />
      <Label fx:id="Lblusernamemyprofile" layoutX="903.0" layoutY="14.0" text="Label" />
      <Button fx:id="btnbacktomain" layoutX="890.0" layoutY="30.0" mnemonicParsing="false" onAction="#backtomain" text="Back" />
    </children>
</AnchorPane>
